        <!-- 秒杀商家 -->
        <typeAlias type="com.bargains.enity.MerchantEntity" alias="merchantEntity"/>
        <typeAlias type="com.bargains.vo.merchantEntity.MsmerchantVo" alias="merchantVo"/>

