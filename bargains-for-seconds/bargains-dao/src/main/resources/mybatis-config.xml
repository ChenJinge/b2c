        <typeAlias type="com.bargains.enity.UserEntity" alias="userEntity"/>
        <typeAlias type="com.bargains.vo.userEntity.MsuserVo" alias="userVo"/>

        <!-- 秒杀商品-->
        <typeAlias type="com.bargains.enity.BargainsEntity" alias="bargainsEntity"/>
        <typeAlias type="com.bargains.vo.bargainsEntity.MsProductVo" alias="bargainsVo"/>

        <!-- 秒杀商品详情-->
        <typeAlias type="com.bargains.enity.ProductEntity" alias="productEntity"/>

        <!-- 秒杀订单-->
        <typeAlias type="com.bargains.enity.OrderEntity" alias="orderEntity"/>
